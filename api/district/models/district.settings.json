{
  "kind": "collectionType",
  "collectionName": "districts",
  "info": {
    "name": "District"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "state_id": {
      "via": "district_state_id",
      "model": "state"
    },
    "is_active": {
      "type": "boolean"
    },
    "taluka_district_id": {
      "via": "district_id",
      "model": "taluka"
    }
  }
}
